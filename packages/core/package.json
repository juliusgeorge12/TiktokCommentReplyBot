{
  "name": "core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/types",
  "dependencies": {
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7"
  },
  "scripts": {
    "migrate": "knex --knexfile ./knexfile.js migrate:latest",
    "rollback": "knex --knexfile ./knexfile.js migrate:rollback",
    "make:migration": "knex --knexfile ./knexfile.js migrate:make",
    "migrate:refresh": "knex --knexfile ./knexfile.js migrate:rollback --all && knex --knexfile ./knexfile.js migrate:latest",
    "build": "tsc",
    "build:knexfile": "tsc ./knexfile.ts"
  }

}
